<Window x:Class="Agricultural_Distributor.WDHome"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Agricultural_Distributor"
        mc:Ignorable="d"
        ResizeMode="NoResize"
        WindowStyle="None"
        Title="WDHome" Height="600" Width="1200" MouseLeftButtonDown="Window_MouseLeftButtonDown">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="240"/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Grid x:Name="mainControl" Grid.Column="0"/>
        <Grid x:Name="mainWork" Grid.Column="1"/>
    </Grid>
</Window>
